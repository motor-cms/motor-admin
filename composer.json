{
  "name": "motor-cms/motor-admin",
  "description": "Motor admin package",
  "license": "MIT",
  "keywords": [
    "motor"
  ],
  "authors": [
    {
      "name": "Reza Esmaili",
      "email": "me@dfox.info"
    }
  ],
  "require": {
    "php": "^8.1",
    "motor-cms/motor-core": "^2.0",
    "php-console/php-console": "^3.1",
    "guzzlehttp/guzzle": "^7.0.1",
    "barryvdh/laravel-ide-helper": "^2.9",
    "intervention/imagecache": "^2.4",
    "league/glide-laravel": "^1.0",
    "dfox288/culpa": "^9.0.2",
    "symfony/intl": "^5.0",
    "spatie/laravel-medialibrary": "^9.0",
    "spatie/laravel-permission": "^5.0",
    "kalnoy/nestedset": "^6.0",
    "laravel/horizon": "^5.0",
    "matthewbdaly/laravel-etag-middleware": "^1.3",
    "darkaonline/l5-swagger": "^8.0",
    "laravel/sanctum": "^2.0"
  },
  "require-dev": {
    "symfony/css-selector": "^5.0",
    "symfony/dom-crawler": "^5.0",
    "laravel/dusk": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Motor\\Admin\\": "src",
      "Tests\\Motor\\Admin\\": "tests",
      "Motor\\Admin\\Database\\Factories\\": "database/factories/",
      "Motor\\Admin\\Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "helpers/motor_admin_helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Motor\\Admin\\Providers\\MotorServiceProvider",
        "Motor\\Admin\\Providers\\MotorAuthServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
